{
    "name": "et-xui-e2e-tests",
    "version": "1.0.0",
    "description": "Employment Tribunal E2E Test",
    "main": "index.js",
    "repository": "git@github.com:hmcts/et-xui-e2e-tests.git",
    "author": "sunday.ayeni@hmcts.net",
    "license": "hmcts",
    "private": true,
    "scripts": {
        "test:fullfunctional": "codeceptjs run --steps -c test/e2e/codecept.conf.js --steps --reporter mocha-multi --grep @RET-BAT",
        "parallel": "codeceptjs run --steps -c test/e2e/codecept.conf.js run-multiple parallel",
        "codeceptjs:headless": "HEADLESS=true codeceptjs run --steps",
        "lint-staged": "lint-staged",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix && prettier --check ./test",
        "codeceptjs:demo": "codeceptjs run --steps -c node_modules/@codeceptjs/examples/codecept.webdriver.conf.js",
        "codeceptjs:demo:headless": "HEADLESS=true codeceptjs run --steps -c node_modules/@codeceptjs/examples/codecept.webdriver.conf.js",
        "codeceptjs:demo:ui": "codecept-ui --app  -c node_modules/@codeceptjs/examples/codecept.webdriver.conf.js",
        "test:crossbrowser": "codeceptjs run-multiple ${BROWSER_GROUP:-'--all'} --steps -c saucelabs.conf.js --steps --reporter mocha-multi --verbose",
        "test:profile": "SAUCE_TUNNEL_NAME=sunnyayeni_tunnel_name yarn test:fullfunctional  --steps -c saucelabs.conf.js --profile sauce:chrome,firefox,safari",
        "prepare": "husky install"
    },
    "lint-staged": {
        "test/**/*.{js,jsx,ts,tsx}": "eslint"
    },
    "dependencies": {
        "@wdio/cli": "7.25.4",
        "@wdio/crossbrowsertesting-service": "7.25.4",
        "@wdio/sauce-service": "7.25.4",
        "@wdio/selenium-standalone-service": "7.25.4",
        "@wdio/types": "7.25.4",
        "codeceptjs-saucehelper": "1.4.0",
        "eslint": "8.26.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-codeceptjs": "1.3.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-mocha": "10.1.0",
        "eslint-plugin-prettier": "4.2.1",
        "faker": "6.6.6",
        "lint-staged": "13.0.3",
        "mocha": "10.0.0",
        "mocha-multi": "1.1.6",
        "mochawesome": "7.1.3",
        "playwright": "^1.27.1",
        "prettier": "2.7.1",
        "typescript": "4.7.4"
    },
    "devDependencies": {
        "@codeceptjs/configure": "0.8.0",
        "@codeceptjs/examples": "1.2.1",
        "@codeceptjs/ui": "0.4.7",
        "codeceptjs": "3.3.6",
        "husky": "8.0.1",
        "webdriverio": "7.25.4"
    }
}
