{
    "name": "et-xui-e2e-tests",
    "version": "1.0.0",
    "description": "Employment Tribunal E2E Test",
    "main": "index.js",
    "repository": "git@github.com:hmcts/et-xui-e2e-tests.git",
    "author": "sunday.ayeni@hmcts.net",
    "license": "hmcts",
    "private": true,
    "scripts": {
        "test:fullfunctional": "codeceptjs run -c codecept.conf.js --verbose --reporter mocha-multi --grep @nightly",
        "test:local": "codeceptjs run -c codecept.conf.js --verbose --reporter mocha-multi",
        "parallel": "codeceptjs run --steps -c codecept.conf.js run-multiple parallel",
        "codeceptjs:headless": "HEADLESS=true codeceptjs run --steps",
        "lint-staged": "lint-staged",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix && prettier --check ./test",
        "fortifyScan": "./src/test/java/gradlew -p src/test/java fortifyScan",
        "codeceptjs:demo": "codeceptjs run --steps -c node_modules/@codeceptjs/examples/codecept.webdriver.conf.js",
        "codeceptjs:demo:headless": "HEADLESS=true codeceptjs run --steps -c node_modules/@codeceptjs/examples/codecept.webdriver.conf.js",
        "codeceptjs:demo:ui": "codecept-ui --app  -c node_modules/@codeceptjs/examples/codecept.webdriver.conf.js",
        "test:crossbrowser": "codeceptjs run -c saucelabs.conf.js --verbose --reporter mocha-multi --grep @RET-XB",
        "test:profile": "SAUCE_TUNNEL_NAME=sunnyayeni_tunnel_name yarn test:fullfunctional  --steps -c saucelabs.conf.js --profile sauce:chrome,firefox,safari",
        "prepare": "husky install"
    },
    "lint-staged": {
        "test/**/*.{js,jsx,ts,tsx}": "eslint"
    },
    "dependencies": {
        "@wdio/cli": "8.22.1",
        "@wdio/crossbrowsertesting-service": "8.22.1",
        "@wdio/sauce-service": "8.22.1",
        "@wdio/selenium-standalone-service": "8.15.0",
        "@wdio/types": "8.21.0",
        "codeceptjs-saucehelper": "1.4.0",
        "codeceptjs-saucelabs": "^5.3.2",
        "eslint": "8.53.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-codeceptjs": "1.3.0",
        "eslint-plugin-import": "2.28.1",
        "eslint-plugin-mocha": "10.2.0",
        "eslint-plugin-prettier": "5.0.1",
        "faker": "6.6.6",
        "lint-staged": "15.0.2",
        "mocha": "10.2.0",
        "mocha-junit-reporter": "^2.2.1",
        "mocha-multi": "1.1.7",
        "mochawesome": "7.1.3",
        "playwright": "^1.27.1",
        "prettier": "3.0.3",
        "typescript": "5.2.2"
    },
    "devDependencies": {
        "@codeceptjs/configure": "0.10.0",
        "@codeceptjs/examples": "1.2.1",
        "@codeceptjs/ui": "0.7.0",
        "codeceptjs": "3.5.7",
        "config": "^3.3.9",
        "husky": "8.0.3",
        "webdriverio": "8.22.1"
    },
    "packageManager": "yarn@3.6.4"
}
